<html lang="en">
  <head>
    <title>Bon de commande pour {{providerName}}</title>
    <style>
      body {
        margin: unset;
        font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
      }

      h1 {
        text-align: center;
        font-size: 1.25rem;
        margin-top: 2rem;
      }

      header {
        display: flex;
        justify-content: space-between;
      }

      .price {
        max-width: 360px;
        margin-left: auto;
        display: flex;
        justify-content: space-between;
      }

      .price > span:first-of-type {
        font-weight: bold;
      }

      .prices {
        margin-top: 1rem;
        border-top: 1px solid rgba(0, 0, 0, 0.108);
      }

      .date {
        padding-top: 1rem;
      }

      .container {
        max-width: 1080px;
        margin: 0 auto;
        padding: 1rem;
        border: 1px solid black;
      }

      h1 span {
        font-style: italic;
      }

      table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
      }

      td,
      th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
      }

      tr:nth-child(even) {
        background-color: #dddddd;
      }
    </style>
  </head>
  <body>
    <div class="container">
      <header>
        <div>
          <img
            width="180px"
            src="data:image/png;base64,"
            alt="logo"
          />
        </div>
        <div class="date"><b>Date de la commande</b> : <span>{{createdAt}}</span></div>
      </header>
      <h1>Bon de commande pour <span>{{providerName}}</span></h1>
      <table>
        <tr>
          <th>Produit</th>
          <th>Unitaire TTC</th>
          <th>Unitaire HT</th>
          <th>Quantité</th>
        </tr>
        {{#orders}}
        <tr>
          <td>{{medicineName}}</td>
          <td>{{priceWithTax}}</td>
          <td>{{priceWithoutTax}}</td>
          <td>{{quantity}}</td>
        </tr>
        {{/orders}}
      </table>
      <div class="prices">
        <p class="price">
          <span>Prix HT</span><span>{{priceWithoutTax}} Ar</span>
        </p>
        <p class="price">
          <span>Prix TTC</span><span>{{priceWithTax}} Ar</span>
        </p>
      </div>
    </div>
  </body>
</html>
